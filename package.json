{
  "name": "react-easy-carousel",
  "version": "0.0.0",
  "keywords": [
    "react",
    "easy",
    "carousel"
  ],
  "main": "dist/index.js",
  "author": "Kennedy Rose <krose@escaladesports.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/escaladesports/react-easy-carousel.git"
  },
  "license": "MIT",
  "scripts": {
    "analyze": "esca-scripts analyze --react --component --input ./src/index.js",
    "build": "esca-scripts build --react --component --babel --input src --output dist --multiple --styledJsx",
    "dev": "esca-scripts dev ---input './dev/dev.js, ./dev/index.html' --react --browser --styledJsx",
    "prepublishOnly": "npm run build",
    "reset": "esca-scripts reset",
    "test": "echo 'no test'",
    "travis": "opn https://travis-ci.org/escaladesports/react-easy-carousel/branches"
  },
  "devDependencies": {
    "esca-scripts": "^0.14.7",
    "opn-cli": "^3.1.0",
    "react": "^16.1.1",
    "react-dom": "^16.1.1"
  },
  "dependencies": {
    "react-animate-x": "^1.3.0",
    "styled-jsx": "^2.2.6"
  }
}
